{
    "name": "dream_qa_watermark_strategies",
    "description": "Watermarking strategies on QA tasks with DREAM model using different deltas and algorithms",
    "base_command": "python -m dmark.llada.only_gen_dream",
    "fixed_params": {
        "model": "Dream-org/Dream-v0-Instruct-7B",
        "output_dir": "results/dream/qa_watermark",
        "vocab_size": 152064,
        "num_samples": 500,
        "bitmap_dir": "bitmaps",
        "steps": 512,
        "gen_length": 256,
        "temperature": 0.2,
        "eps": 1e-3,
        "top_p": 0.95,
        "ratio": 0.5,
        "key": 42,
        "prebias": false,
        "minimum_output_token": 200,
        "dataset": "sentence-transformers/eli5",
        "output_history": false,
        "return_dict_in_generate": true
    },
    "variable_params": {
        "delta": [1.0, 2.0, 5.0, 10.0],
        "strategy": ["normal", "predict", "reverse"]
    }
}