{
    "name": "baseline_no_watermark",
    "description": "Baseline LLaDA experiments without watermarking",
    "base_command": "python -m dmark.llada.only_gen",
    "fixed_params": {
        "model": "GSAI-ML/LLaDA-8B-Instruct",
        "output_dir": "results/llada/baseline",
        "vocab_size": 126464,
        "num_samples": 1000,
        "minimum_output_token": 200,
        "steps": 256,
        "gen_length": 256,
        "block_length": 32,
        "cfg_scale": 0.0,
        "temperature": 0.0
    },
    "variable_params": {
        "dataset": ["sentence-transformers/eli5", "allenai/c4", "openai/gsm8k"],
        "remasking": ["low_confidence", "random", "right_to_left", "left_to_right"]
    }
}