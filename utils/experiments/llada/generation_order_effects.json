{
    "name": "generation_order_effects",
    "description": "Investigate how generation order (remasking) affects watermark effectiveness across different strategies",
    "base_command": "python -m dmark.llada.only_gen",
    "fixed_params": {
        "model": "GSAI-ML/LLaDA-8B-Instruct",
        "dataset": "sentence-transformers/eli5",
        "output_dir": "results/llada/generation_order",
        "vocab_size": 126464,
        "num_samples": 500,
        "minimum_output_token": 200,
        "steps": 256,
        "gen_length": 256,
        "block_length": 32,
        "cfg_scale": 0.0,
        "temperature": 0.0,
        "ratio": 0.5,
        "delta": 2.0,
        "key": 42,
        "prebias": false,
        "bitmap_dir": "bitmaps"
    },
    "variable_params": {
        "remasking": ["low_confidence", "random", "right_to_left", "left_to_right"],
        "strategy": ["normal", "predict", "bidirectional"]
    }
}