{
    "name": "math_watermark_strategies",
    "description": "Math problem solving (GSM8K) with different watermark strategies",
    "base_command": "python -m dmark.llada.only_gen",
    "fixed_params": {
        "model": "GSAI-ML/LLaDA-8B-Instruct",
        "output_dir": "results/llada/math_watermark",
        "vocab_size": 126464,
        "num_samples": 500,
        "bitmap_dir": "bitmaps",
        "steps": 256,
        "gen_length": 256,
        "block_length": 32,
        "temperature": 0.0,
        "cfg_scale": 0.0,
        "key": 42,
        "prebias": false,
        "minimum_output_token": 200,
        "dataset": "openai/gsm8k"
    },
    "variable_params": {
        "delta": [0.5, 1.0, 2.0, 5.0, 10.0],
        "remasking": ["low_confidence", "random", "right_to_left", "left_to_right"],
        "strategy": ["normal", "predict", "bidirectional", "predict-bidirectional"],
        "ratio": [0.5, 0.75, 0.25]
    }
}