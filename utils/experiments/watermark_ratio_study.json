{
    "name": "watermark_ratio_study",
    "description": "Study the effect of different green list ratios on watermarking",
    "base_command": "python -m dmark.llada.only_gen",
    "fixed_params": {
        "model": "GSAI-ML/LLaDA-8B-Instruct",
        "output_dir": "results/llada/ratio_study",
        "vocab_size": 126464,
        "num_samples": 500,
        "dataset": "sentence-transformers/eli5",
        "steps": 256,
        "gen_length": 256,
        "block_length": 32,
        "temperature": 0.0,
        "cfg_scale": 0.0,
        "remasking": "low_confidence",
        "delta": 2.0,
        "key": 42,
        "prebias": false,
        "minimum_output_token": 200,
        "bitmap_dir": "."
    },
    "variable_params": {
        "ratio": [0.25, 0.5, 0.75],
        "strategy": ["normal", "predict", "reverse"]
    }
}